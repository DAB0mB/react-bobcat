{
  "name": "react-bobcat",
  "description": "A library for testing navigation flows in React",
  "version": "0.1.1",
  "license": "MIT",
  "author": "DAB0mB",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dab0mb/react-bobcat.git"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "prepublishOnly": "yarn build",
    "lint": "eslint --ignore-pattern '!src/**/*.ts' --fix"
  },
  "peerDependencies": {
    "react": "16.8"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "git add"
    ]
  },
  "keywords": [
    "e2e",
    "react",
    "test"
  ]
}
